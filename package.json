{
  "name": "material-react-components",
  "version": "1.2.0",
  "description": "A collection of React components that implement Google's material design specification.",
  "main": "dist/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "sideEffects": false,
  "keywords": [
    "react",
    "react-component",
    "material design",
    "material-ui"
  ],
  "scripts": {
    "build": "npm-run-all build:dist:* build:es:*",
    "build:dist:js": "webpack --config webpack.config.dist.js -p",
    "build:dist:main": "babel src/index.js > dist/index.js",
    "build:docs": "NODE_ENV=production webpack --config webpack.config.docs.js --env.docs",
    "build:es:css": "postcss src/**/*.css --base src --dir es",
    "build:es:js": "NODE_ENV=production babel src --ignore *.spec.js --out-dir es",
    "build:es:main": "cp src/index.js es/index.js",
    "build:ssr": "webpack --config webpack.config.ssr.js",
    "coverage:open": "open .coverage/lcov-report/index.html",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint src/**/*.css",
    "lint:js": "eslint src/**/*.js",
    "start": "webpack-dev-server --config webpack.config.docs.js --env.dev --content-base docs",
    "test": "npm run test:unit && npm run build:ssr && npm run test:ssr",
    "test:ssr": "mocha .tmp/main.js",
    "test:unit": "karma start",
    "test:unit:chrome": "SOURCEMAPS=false karma start --browsers Chrome --no-single-run",
    "test:unit:safari": "SOURCEMAPS=false karma start --browsers Safari --no-single-run"
  },
  "author": "Peter Mikitsh",
  "license": "MIT",
  "peerDependencies": {
    "react": "^15.5.4 || ^16.0.0",
    "react-dom": "^15.5.4 || ^16.0.0"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "deep-assign": "2.0.0",
    "keycode": "2.2.0",
    "material-design-icons": "3.0.1",
    "react-scrollbar-size": "2.1.0",
    "react-transition-group": "1.2.1",
    "tinycolor2": "1.4.1",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-rc.1",
    "@babel/core": "7.0.0-rc.1",
    "@babel/plugin-proposal-class-properties": "7.0.0-rc.1",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-rc.1",
    "@babel/preset-env": "7.0.0-rc.1",
    "@babel/preset-react": "7.0.0-rc.1",
    "autoprefixer": "9.1.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.6",
    "babel-loader": "7.1.5",
    "babel-plugin-istanbul": "4.1.6",
    "babel-plugin-transform-define": "1.3.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.14",
    "babel-polyfill": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "core-js": "2.5.7",
    "css-loader": "1.0.0",
    "enzyme": "3.4.1",
    "enzyme-adapter-react-16": "1.2.0",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.0",
    "html-webpack-plugin": "3.2.0",
    "isomorphic-style-loader": "4.0.0",
    "karma": "3.0.0",
    "karma-browserstack-launcher": "1.3.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.2",
    "karma-coveralls": "2.0.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-safari-launcher": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "3.0.0",
    "lodash": "4.17.10",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.3",
    "postcss-cli": "6.0.0",
    "postcss-loader": "3.0.0",
    "postcss-simple-vars": "4.1.0",
    "prismjs": "1.15.0",
    "prop-types": "15.6.2",
    "raw-loader": "0.5.1",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-hot-loader": "4.3.4",
    "react-router-dom": "4.3.1",
    "react-svg-loader": "2.1.0",
    "react-tap-event-plugin": "3.0.3",
    "react-test-renderer": "16.4.2",
    "sinon": "6.1.5",
    "style-loader": "0.22.1",
    "stylelint": "9.4.0",
    "stylelint-config-standard": "18.2.0",
    "webpack": "4.16.5",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5",
    "webpack-node-externals": "1.7.2"
  }
}
