{
  "name": "material-react-components",
  "version": "0.0.1-0",
  "description": "A collection of React components that implement Google's material design specification.",
  "main": "./dist/index.js",
  "module": "./es/index.js",
  "jsnext:main": "./es/index.js",
  "scripts": {
    "build": "npm-run-all build:dist:* build:es:*",
    "build:dist:js": "webpack --config webpack.config.dist.js -p",
    "build:dist:main": "babel ./src/index.js > ./dist/index.js",
    "build:docs": "NODE_ENV=production webpack --config webpack.config.docs.js --env.docs",
    "build:es:css": "postcss src/**/*.css --base src --dir es",
    "build:es:js": "babel ./src --ignore *.spec.js --out-dir ./es",
    "build:es:main": "cp ./src/index.js ./es/index.js",
    "build:ssr": "webpack --config webpack.config.ssr.js",
    "coverage:open": "open .coverage/lcov-report/index.html",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint src/**/*.css",
    "lint:js": "eslint src/**/*.js",
    "start": "webpack-dev-server --config webpack.config.docs.js --env.dev --content-base docs",
    "test": "npm run test:unit && npm run build:ssr && npm run test:ssr",
    "test:ssr": "mocha .tmp/main.js",
    "test:unit": "karma start",
    "test:unit:chrome": "SOURCEMAPS=false karma start --browsers Chrome --no-single-run"
  },
  "author": "Peter Mikitsh",
  "license": "MIT",
  "peerDependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "dependencies": {
    "babel-cli": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-env": "1.5.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-optimize": "1.0.1",
    "classnames": "2.2.5",
    "keycode": "2.1.9",
    "material-design-icons": "3.0.1",
    "react-transition-group": "1.1.3",
    "tinycolor2": "1.4.1",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-plugin-empower-assert": "1.3.0",
    "babel-plugin-espower": "2.3.2",
    "babel-plugin-istanbul": "4.1.4",
    "babel-polyfill": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.0.0",
    "css-loader": "0.28.4",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "html-webpack-plugin": "2.28.0",
    "isomorphic-style-loader": "2.0.0",
    "karma": "1.7.0",
    "karma-browserstack-launcher": "1.2.0",
    "karma-chrome-launcher": "2.1.1",
    "karma-coverage": "1.1.1",
    "karma-coveralls": "1.1.2",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.3",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.3",
    "lodash": "4.17.4",
    "mocha": "3.4.2",
    "npm-run-all": "4.0.2",
    "phantomjs-polyfill-object-assign": "0.0.2",
    "postcss-cli": "4.1.0",
    "postcss-loader": "2.0.5",
    "postcss-simple-vars": "4.0.0",
    "power-assert": "1.4.2",
    "prismjs": "1.6.0",
    "prop-types": "15.5.10",
    "raw-loader": "0.5.1",
    "react": "15.5.4",
    "react-addons-test-utils": "15.5.1",
    "react-dom": "15.5.4",
    "react-hot-loader": "3.0.0-beta.7",
    "react-router-dom": "4.1.1",
    "react-svg-loader": "2.0.0-alpha.1",
    "react-tap-event-plugin": "2.0.1",
    "react-test-renderer": "15.5.4",
    "sinon": "2.3.2",
    "style-loader": "0.18.1",
    "stylelint": "7.10.1",
    "stylelint-config-standard": "16.0.0",
    "webpack": "3.0.0",
    "webpack-dev-server": "2.4.5",
    "webpack-node-externals": "1.6.0"
  }
}
