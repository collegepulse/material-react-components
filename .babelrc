{
  "env": {
    "production": {
      "plugins": [
        "transform-react-remove-prop-types",
        "transform-runtime",
        [
          "transform-define", {
            "__TEST__": false
          }
        ]
      ]
    }
  },
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-object-assign",
    [
      "transform-runtime", {
        helpers: false,
        polyfill: false,
        regenerator: true
      }
    ]
  ],
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": ["last 2 versions"]
        },
        "useBuiltIns": true
      }
    ],
    "es2015",
    "react"
  ]
}
